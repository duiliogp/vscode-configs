[
  { "key": "F5", "command": "extension.multiCommand.execute", "args": { "command": "multiCommand.clearTerminalandDebugConsole" }, "when": "editorTextFocus" },
  { "key": "shift+F5", "command": "code-runner.stop", "when": "editorTextFocus" },
  { "key": "ctrl+m", "command": "workbench.action.toggleStatusbarVisibility" },
  { "key": "ctrl+L", "command": "workbench.action.toggleActivityBarVisibility" },
  { "key": "F12", "command": "-editor.action.goToDeclaration", "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
  { "key": "ctrl+[IntlRo]", "command": "editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" },
  { "key": "shift+enter", "command": "editor.action.insertLineAfter", "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+i", "command": "workbench.action.showAllSymbols" },
  { "key": "ctrl+shift+i", "command": "extension.wrapTag", "when": "editorTextFocus" },
  { "key": "ctrl+'", "command": "workbench.action.togglePanel" },
  { "key": "ctrl+j", "command": "-workbench.action.togglePanel" }
]